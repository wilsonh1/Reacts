{
    "name": "Reacts",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Test",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "page_action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "permissions": ["storage","activeTab","declarativeContent"]
}
